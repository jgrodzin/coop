%header.centered-navigation
  .centered-navigation-wrapper
    %a.mobile-logo{}
      %img{:alt => "Logo image", :src => "assets/coop-logo.jpg"}
    %a.centered-navigation-menu-button{:href => ""} MENU
    %ul.centered-navigation-menu
      - if signed_in?
        %li.nav-link
          %a{:href => root_path} Home
        %li.nav-link
          %a{:href => events_path} Events
        %li.nav-link
          %a{:href => teams_path} Team
        %li.nav-link
          %a{:href => members_path} Members
        %li.nav-link
          %a{:href => vendors_path} Vendors

          %li.nav-link.sign-out
            =link_to "Sign Out", destroy_member_session_path, method: :delete, class: 'sign-out'
